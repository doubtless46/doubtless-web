{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "components/*": ["components/*"],
      "pages/*": ["pages/*"],
      "constants/*": ["constants/*"],
      "styles/*": ["styles/*"],
      "public/*": ["public/*"],
      "utils/*": ["utils/*"],
      "static/*": ["static/*"],
      "features/*": ["features/*"],
      "hooks/*": ["hooks/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/AnimatedText.tsx"],
  "exclude": ["node_modules"]
}
